{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strictPropertyInitialization": false,
    "types": ["jest", "node", "@testing-library/jest-dom"]
  },
  "include": [
    "src",
    "../src/backend/sketchProcessing/editorObjectSchemas.ts",
    "../src/backend/sketchProcessing/layoutCraftTreeConverter.ts",
    "../src/backend/sketchProcessing/oaApi.ts",
    "../src/backend/sketchProcessing/sketchStructureToNodeTree.ts",
    "../src/backend/sketchProcessing/sketchUpload.ts",
    "src/types"
  ]
}