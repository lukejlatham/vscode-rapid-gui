{
  "name": "vscode-test-2",
  "displayName": "vscode-test-2",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vscodeTest2Sidebar",
    "onCommand:vscode-test-2.openWebView",
    "onCommand:vscode-test-2.refreshTree"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscodeTest2Sidebar",
          "title": "Test Sidebar",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "vscodeTest2Sidebar": [
        {
          "id": "vscodeTest2View",
          "name": "Test View"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-test-2.openWebView",
        "title": "Open WebView"
      },
      {
        "command": "vscode-test-2.refreshTree",
        "title": "Refresh Tree"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js && npm run build:react",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "build:react": "webpack --mode production"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/react": "^18.3.3",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "babel-loader": "^9.1.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
