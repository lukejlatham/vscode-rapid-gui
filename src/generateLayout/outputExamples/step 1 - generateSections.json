{
  "theme": "discordpurple",
  "fontFamily": "IBM",
  "sections": [
    {
      "section": "Header",
      "xPosition": 0,
      "yPosition": 0,
      "width": 10,
      "height": 1,
      "flexDirection": "row",
      "backgroundColor": "Main",
      "children": [
        {
          "element": "Label",
          "text": "Visual Studio Code",
          "bold": true,
          "fontColor": "LightAccent",
          "fontSize": "Title"
        }
      ]
    },
    {
      "section": "Sidebar",
      "xPosition": 0,
      "yPosition": 1,
      "width": 1,
      "height": 9,
      "flexDirection": "column",
      "backgroundColor": "DarkAccent",
      "children": [
        {
          "element": "Button",
          "vscIcon": "home",
          "backgroundColor": "Main",
          "width": 10,
          "height": 5
        },
        {
          "element": "Button",
          "vscIcon": "settings",
          "backgroundColor": "Main",
          "width": 10,
          "height": 5
        },
        {
          "element": "Button",
          "vscIcon": "extensions",
          "backgroundColor": "Main",
          "width": 10,
          "height": 5
        },
        {
          "element": "Button",
          "vscIcon": "source-control",
          "backgroundColor": "Main",
          "width": 10,
          "height": 5
        },
        {
          "element": "Button",
          "vscIcon": "debug",
          "backgroundColor": "Main",
          "width": 10,
          "height": 5
        }
      ]
    },
    {
      "section": "Main Editor",
      "xPosition": 1,
      "yPosition": 1,
      "width": 6,
      "height": 6,
      "flexDirection": "column",
      "backgroundColor": "LightAccent",
      "children": [{ "element": "Input", "placeholder": "Type your code here..." }]
    },
    {
      "section": "Terminal",
      "xPosition": 1,
      "yPosition": 7,
      "width": 6,
      "height": 3,
      "flexDirection": "column",
      "backgroundColor": "DarkAccent",
      "children": [{ "element": "Text", "fontColor": "LightAccent" }]
    },
    {
      "section": "Right Panel",
      "xPosition": 7,
      "yPosition": 1,
      "width": 3,
      "height": 9,
      "flexDirection": "column",
      "backgroundColor": "DarkAccent",
      "children": [
        { "element": "Checkboxes", "optionLabels": ["Auto-save", "Format on save", "Use git"] },
        { "element": "Slider", "header": "Zoom", "backgroundColor": "Main" },
        { "element": "Dropdown", "optionLabels": ["JavaScript", "Python", "C++", "HTML", "CSS"] },
        { "element": "Icon", "vscIcon": "file", "iconSize": 32, "iconColor": "Main" }
      ]
    }
  ]
}
