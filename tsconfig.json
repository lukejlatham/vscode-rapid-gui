{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["ES6", "DOM"],
    "allowJs": true,
    "outDir": "out",
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react"  // Add this line to enable JSX
  },
  "include": ["src"],
  "exclude": ["node_modules", ".vscode-test"]
}
